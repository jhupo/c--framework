{
    "default":{
        "destination": "TextFile",
        "category": "Root",
        "pattern": "[%Y-%m-%d %H:%M:%S][%l][%t][%n][%!][%s:%#]%v",
        "path" : "logs/logger.log",
        "level": "debug",
        "fileSize": 10485760,
        "maxFile" : 10
    },
    "flush":{
        "level": "info",
        "interval": 3 
    }
}
